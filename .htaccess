<IfModule mod_rewrite.c>

	RewriteEngine On

	RewriteBase /sik_grandmutiara

	RewriteCond %{REQUEST_FILENAME} !-f

	RewriteCond %{REQUEST_FILENAME} !-d

	RewriteRule ^(.*)$ /sik_grandmutiara/index.php/$1 [L]

</IfModule>

<IfModule !mod_rewrite.c>

	ErrorDocument 404 /sik_grandmutiara/index.php

</IfModule>